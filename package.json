{
  "name": "template-page-editor",
  "version": "1.0.0",
  "description": "A modern web application for creating and editing template pages with a user-friendly interface",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --mode production",
    "build:server": "babel src/server -d dist/server",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "template",
    "editor",
    "web",
    "page-builder",
    "cms"
  ],
  "author": "James Lochran",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jameslochran/template-page-editor.git"
  },
  "bugs": {
    "url": "https://github.com/jameslochran/template-page-editor/issues"
  },
  "homepage": "https://github.com/jameslochran/template-page-editor#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-core": "^6.26.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
